{
  "name": "eachless",
  "version": "1.0.2",
  "description": "flat the forEach hell",
  "module": "./lib",
  "main": "./lib",
  "scripts": {
    "clean": "rimraf lib",
    "test": "npm run lint && npm run cover",
    "test:only": "mocha --require @babel/register --require @babel/core --recursive",
    "cover": "nyc --check-coverage --lines 80 npm run test:only",
    "lint": "eslint src test",
    "build": "webpack --mode=production --no-progress --hide-modules --config=webpack.config.js",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apotox/eachless"
  },
  "keywords": [
    "foreach",
    "nested",
    "generator",
    "eachless"
  ],
  "author": "safi eddine bouhentala",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/register": "^7.15.3",
    "babel-loader": "^8.2.2",
    "chai": "^4.1.2",
    "cross-env": "^5.2.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^6.1.3",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "files": [
    "src",
    "lib"
  ]
}
